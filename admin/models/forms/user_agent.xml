<?xml version="1.0" encoding="utf-8"?>
<form
	addrulepath="/administrator/components/com_pro_critical/models/rules"
	addfieldpath="/administrator/components/com_pro_critical/models/fields"
>
	<fieldset name="details">
		<!-- [Fields 492] Default Fields. -->
		<!-- [Fields 493] Id Field. Type: Text (joomla) -->
		<field
			name="id"
			type="text" class="readonly" label="JGLOBAL_FIELD_ID_LABEL"
			description ="JGLOBAL_FIELD_ID_DESC" size="10" default="0"
			readonly="true"
		/>
		<!-- [Fields 509] Date Created Field. Type: Calendar (joomla) -->
		<field
			name="created"
			type="calendar"
			label="COM_PRO_CRITICAL_USER_AGENT_CREATED_DATE_LABEL"
			description="COM_PRO_CRITICAL_USER_AGENT_CREATED_DATE_DESC"
			size="22"
			format="%Y-%m-%d %H:%M:%S"
			filter="user_utc"
		/>
		<!-- [Fields 529] User Created Field. Type: User (joomla) -->
		<field
			name="created_by"
			type="user"
			label="COM_PRO_CRITICAL_USER_AGENT_CREATED_BY_LABEL"
			description="COM_PRO_CRITICAL_USER_AGENT_CREATED_BY_DESC"
		/>
		<!-- [Fields 546] Published Field. Type: List (joomla) -->
		<field name="published" type="list" label="JSTATUS"
			description="JFIELD_PUBLISHED_DESC" class="chzn-color-state"
			filter="intval" size="1" default="1" >
			<option value="1">
				JPUBLISHED</option>
			<option value="0">
				JUNPUBLISHED</option>
			<option value="2">
				JARCHIVED</option>
			<option value="-2">
				JTRASHED</option>
		</field>
		<!-- [Fields 569] Date Modified Field. Type: Calendar (joomla) -->
		<field name="modified" type="calendar" class="readonly"
			label="COM_PRO_CRITICAL_USER_AGENT_MODIFIED_DATE_LABEL" description="COM_PRO_CRITICAL_USER_AGENT_MODIFIED_DATE_DESC"
			size="22" readonly="true" format="%Y-%m-%d %H:%M:%S" filter="user_utc" />
		<!-- [Fields 579] User Modified Field. Type: User (joomla) -->
		<field name="modified_by" type="user"
			label="COM_PRO_CRITICAL_USER_AGENT_MODIFIED_BY_LABEL"
			description="COM_PRO_CRITICAL_USER_AGENT_MODIFIED_BY_DESC"
			class="readonly"
			readonly="true"
			filter="unset"
		/>
		<!-- [Fields 611] Ordering Field. Type: Numbers (joomla) -->
		<field
			name="ordering"
			type="number"
			class="inputbox validate-ordering"
			label="COM_PRO_CRITICAL_USER_AGENT_ORDERING_LABEL"
			description=""
			default="0"
			size="6"
			required="false"
		/>
		<!-- [Fields 632] Version Field. Type: Text (joomla) -->
		<field
			name="version"
			type="text"
			class="readonly"
			label="COM_PRO_CRITICAL_USER_AGENT_VERSION_LABEL"
			description="COM_PRO_CRITICAL_USER_AGENT_VERSION_DESC"
			size="6"
			readonly="true"
			filter="unset"
		/>
		<!-- [Fields 683] Dynamic Fields. -->
		<!-- [Fields 1465] Ua Field. Type: Text. (joomla) -->
		<field
			type="text"
			name="ua"
			label="COM_PRO_CRITICAL_USER_AGENT_UA_LABEL"
			size="100"
			default=""
			description="COM_PRO_CRITICAL_USER_AGENT_UA_DESCRIPTION"
			class="text_area"
			readonly="false"
			disabled="false"
			required="true"
			filter="RAW"
			message="COM_PRO_CRITICAL_USER_AGENT_UA_MESSAGE"
			hint="COM_PRO_CRITICAL_USER_AGENT_UA_HINT"
			autocomplete="off"
		/>
		<!-- [Fields 1712] Type_device_id Field. Type: Typedeviceid. (custom) -->
		<field
			type="typedeviceid"
			name="type_device_id"
			label="COM_PRO_CRITICAL_USER_AGENT_TYPE_DEVICE_ID_LABEL"
			description="COM_PRO_CRITICAL_USER_AGENT_TYPE_DEVICE_ID_DESCRIPTION"
			class="list_class"
			multiple="false"
			default="0"
			required="false"
			readonly="false"
			disabled="false"
			button="true"
		/>
		<!-- [Fields 1712] Type_browser_id Field. Type: Typebrowserid. (custom) -->
		<field
			type="typebrowserid"
			name="type_browser_id"
			label="COM_PRO_CRITICAL_USER_AGENT_TYPE_BROWSER_ID_LABEL"
			description="COM_PRO_CRITICAL_USER_AGENT_TYPE_BROWSER_ID_DESCRIPTION"
			class="list_class"
			multiple="false"
			default="0"
			required="true"
			readonly="false"
			disabled="false"
			button="true"
		/>
		<!-- [Fields 1465] Brand Field. Type: Text. (joomla) -->
		<field
			type="text"
			name="brand"
			label="COM_PRO_CRITICAL_USER_AGENT_BRAND_LABEL"
			maxlength="50"
			default=""
			description="COM_PRO_CRITICAL_USER_AGENT_BRAND_DESCRIPTION"
			class="text_area"
			readonly="false"
			disabled="false"
			required="false"
			filter="STRING"
			message="COM_PRO_CRITICAL_USER_AGENT_BRAND_MESSAGE"
			hint="COM_PRO_CRITICAL_USER_AGENT_BRAND_HINT"
			autocomplete="on"
		/>
		<!-- [Fields 1465] Name Field. Type: Text. (joomla) -->
		<field
			type="text"
			name="name"
			label="COM_PRO_CRITICAL_USER_AGENT_NAME_LABEL"
			maxlength="50"
			default=""
			description="COM_PRO_CRITICAL_USER_AGENT_NAME_DESCRIPTION"
			class="text_area"
			readonly="false"
			disabled="false"
			required="false"
			filter="STRING"
			message="COM_PRO_CRITICAL_USER_AGENT_NAME_MESSAGE"
			hint="COM_PRO_CRITICAL_USER_AGENT_NAME_HINT"
			autocomplete="on"
		/>
		<!-- [Fields 1712] Browser_id_list Field. Type: Browseridfield. (custom) -->
		<field
			type="browseridfield"
			name="browser_id_list"
			label="COM_PRO_CRITICAL_USER_AGENT_BROWSER_ID_LIST_LABEL"
			description="COM_PRO_CRITICAL_USER_AGENT_BROWSER_ID_LIST_DESCRIPTION"
			class="list_class"
			multiple="false"
			default="0"
			required="false"
			readonly="false"
			disabled="false"
			button="true"
		/>
		<!-- [Fields 1712] Os_list Field. Type: Oslist. (custom) -->
		<field
			type="oslist"
			name="os_list"
			label="COM_PRO_CRITICAL_USER_AGENT_OS_LIST_LABEL"
			description="COM_PRO_CRITICAL_USER_AGENT_OS_LIST_DESCRIPTION"
			class="list_class"
			multiple="false"
			default="0"
			required="false"
			readonly="false"
			disabled="false"
			button="true"
		/>
		<!-- [Fields 1465] Url Field. Type: Text. (joomla) -->
		<field
			type="text"
			name="url"
			label="COM_PRO_CRITICAL_USER_AGENT_URL_LABEL"
			maxlength="255"
			default=""
			description="COM_PRO_CRITICAL_USER_AGENT_URL_DESCRIPTION"
			class="text_area"
			readonly="false"
			disabled="false"
			required="false"
			filter="STRING"
			message="COM_PRO_CRITICAL_USER_AGENT_URL_MESSAGE"
			hint="COM_PRO_CRITICAL_USER_AGENT_URL_HINT"
			autocomplete="on"
		/>
	</fieldset>
	
	<!-- [Interpretation 12352] Access Control Fields. -->
	<fieldset name="accesscontrol">
		<!-- [Interpretation 12354] Asset Id Field. Type: Hidden (joomla) -->
		<field
			name="asset_id"
			type="hidden"
			filter="unset"
		/>
		<!-- [Interpretation 12360] Rules Field. Type: Rules (joomla) -->
		<field
			name="rules"
			type="rules"
			label="Permissions in relation to this user_agent"
			translate_label="false"
			filter="rules"
			validate="rules"
			class="inputbox"
			component="com_pro_critical"
			section="user_agent"
		/>
	</fieldset>
</form>