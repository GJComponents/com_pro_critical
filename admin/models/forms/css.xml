<?xml version="1.0" encoding="utf-8"?>
<form
	addrulepath="/administrator/components/com_pro_critical/models/rules"
	addfieldpath="/administrator/components/com_pro_critical/models/fields"
>
	<fieldset name="details">
		<!-- [Fields 492] Default Fields. -->
		<!-- [Fields 493] Id Field. Type: Text (joomla) -->
		<field
			name="id"
			type="text" class="readonly" label="JGLOBAL_FIELD_ID_LABEL"
			description ="JGLOBAL_FIELD_ID_DESC" size="10" default="0"
			readonly="true"
		/>
		<!-- [Fields 509] Date Created Field. Type: Calendar (joomla) -->
		<field
			name="created"
			type="calendar"
			label="COM_PRO_CRITICAL_CSS_CREATED_DATE_LABEL"
			description="COM_PRO_CRITICAL_CSS_CREATED_DATE_DESC"
			size="22"
			format="%Y-%m-%d %H:%M:%S"
			filter="user_utc"
		/>
		<!-- [Fields 529] User Created Field. Type: User (joomla) -->
		<field
			name="created_by"
			type="user"
			label="COM_PRO_CRITICAL_CSS_CREATED_BY_LABEL"
			description="COM_PRO_CRITICAL_CSS_CREATED_BY_DESC"
		/>
		<!-- [Fields 546] Published Field. Type: List (joomla) -->
		<field name="published" type="list" label="JSTATUS"
			description="JFIELD_PUBLISHED_DESC" class="chzn-color-state"
			filter="intval" size="1" default="1" >
			<option value="1">
				JPUBLISHED</option>
			<option value="0">
				JUNPUBLISHED</option>
			<option value="2">
				JARCHIVED</option>
			<option value="-2">
				JTRASHED</option>
		</field>
		<!-- [Fields 569] Date Modified Field. Type: Calendar (joomla) -->
		<field name="modified" type="calendar" class="readonly"
			label="COM_PRO_CRITICAL_CSS_MODIFIED_DATE_LABEL" description="COM_PRO_CRITICAL_CSS_MODIFIED_DATE_DESC"
			size="22" readonly="true" format="%Y-%m-%d %H:%M:%S" filter="user_utc" />
		<!-- [Fields 579] User Modified Field. Type: User (joomla) -->
		<field name="modified_by" type="user"
			label="COM_PRO_CRITICAL_CSS_MODIFIED_BY_LABEL"
			description="COM_PRO_CRITICAL_CSS_MODIFIED_BY_DESC"
			class="readonly"
			readonly="true"
			filter="unset"
		/>
		<!-- [Fields 611] Ordering Field. Type: Numbers (joomla) -->
		<field
			name="ordering"
			type="number"
			class="inputbox validate-ordering"
			label="COM_PRO_CRITICAL_CSS_ORDERING_LABEL"
			description=""
			default="0"
			size="6"
			required="false"
		/>
		<!-- [Fields 632] Version Field. Type: Text (joomla) -->
		<field
			name="version"
			type="text"
			class="readonly"
			label="COM_PRO_CRITICAL_CSS_VERSION_LABEL"
			description="COM_PRO_CRITICAL_CSS_VERSION_DESC"
			size="6"
			readonly="true"
			filter="unset"
		/>
		<!-- [Fields 683] Dynamic Fields. -->
		<!-- [Fields 1465] Query_string Field. Type: Text. (joomla) -->
		<field
			type="text"
			name="query_string"
			label="COM_PRO_CRITICAL_CSS_QUERY_STRING_LABEL"
			maxlength="255"
			default=""
			description="COM_PRO_CRITICAL_CSS_QUERY_STRING_DESCRIPTION"
			class="text_area span8"
			readonly="true"
			disabled="false"
			required="false"
			filter="RAW"
			autocomplete="on"
		/>
		<!-- [Fields 1712] Option Field. Type: Criticalcssoption. (custom) -->
		<field
			type="criticalcssoption"
			name="option"
			label="COM_PRO_CRITICAL_CSS_OPTION_LABEL"
			description="COM_PRO_CRITICAL_CSS_OPTION_DESCRIPTION"
			class="list_class"
			multiple="false"
			default="0"
			required="true"
			readonly="false"
			disabled="false"
			button="true"
		/>
		<!-- [Fields 1712] View Field. Type: Criticalcssview. (custom) -->
		<field
			type="criticalcssview"
			name="view"
			label="COM_PRO_CRITICAL_CSS_VIEW_LABEL"
			description="COM_PRO_CRITICAL_CSS_VIEW_DESCRIPTION"
			class="list_class"
			multiple="false"
			default="0"
			required="true"
			readonly="false"
			disabled="false"
			button="true"
		/>
		<!-- [Fields 1465] Alias Field. Type: Text. (joomla) -->
		<field
			type="text"
			name="alias"
			label="COM_PRO_CRITICAL_CSS_ALIAS_LABEL"
			maxlength="255"
			default=""
			description="COM_PRO_CRITICAL_CSS_ALIAS_DESCRIPTION"
			class="text_area time_lock"
			readonly="true"
			disabled="false"
			required="false"
			filter="INT"
			hint="JFIELD_ALIAS_PLACEHOLDER"
			autocomplete="on"
		/>
		<!-- [Fields 1465] Critical_css_code Field. Type: Textarea. (joomla) -->
		<field
			type="textarea"
			name="critical_css_code"
			label="COM_PRO_CRITICAL_CSS_CRITICAL_CSS_CODE_LABEL"
			rows="10"
			cols="5"
			default=""
			description="COM_PRO_CRITICAL_CSS_CRITICAL_CSS_CODE_DESCRIPTION"
			class="text_area span8"
			filter="raw"
			required="false"
			readonly="true"
			disabled="false"
		/>
		<!-- [Fields 1465] Photo_afte Field. Type: Text. (joomla) -->
		<field
			type="text"
			name="photo_afte"
			label="COM_PRO_CRITICAL_CSS_PHOTO_AFTE_LABEL"
			maxlength="1024"
			default=""
			description="COM_PRO_CRITICAL_CSS_PHOTO_AFTE_DESCRIPTION"
			class="text_area"
			readonly="true"
			disabled="false"
			required="false"
			filter="RAW"
			autocomplete="off"
		/>
		<!-- [Fields 1465] Photo_before Field. Type: Text. (joomla) -->
		<field
			type="text"
			name="photo_before"
			label="COM_PRO_CRITICAL_CSS_PHOTO_BEFORE_LABEL"
			maxlength="1024"
			default=""
			description="COM_PRO_CRITICAL_CSS_PHOTO_BEFORE_DESCRIPTION"
			class="text_area"
			readonly="true"
			disabled="false"
			required="false"
			filter="RAW"
			message="COM_PRO_CRITICAL_CSS_PHOTO_BEFORE_MESSAGE"
			autocomplete="off"
		/>
		<!-- [Fields 1712] Type_device_id Field. Type: Typedeviceid. (custom) -->
		<field
			type="typedeviceid"
			name="type_device_id"
			label="COM_PRO_CRITICAL_CSS_TYPE_DEVICE_ID_LABEL"
			description="COM_PRO_CRITICAL_CSS_TYPE_DEVICE_ID_DESCRIPTION"
			class="list_class"
			multiple="false"
			default="0"
			required="false"
			readonly="false"
			disabled="false"
			button="true"
		/>
		<!-- [Fields 1465] User_agent_id Field. Type: Text. (joomla) -->
		<field
			type="text"
			name="user_agent_id"
			label="COM_PRO_CRITICAL_CSS_USER_AGENT_ID_LABEL"
			maxlength="50"
			default=""
			description="COM_PRO_CRITICAL_CSS_USER_AGENT_ID_DESCRIPTION"
			class="text_area"
			readonly="false"
			disabled="false"
			required="false"
			filter="INT"
			message="COM_PRO_CRITICAL_CSS_USER_AGENT_ID_MESSAGE"
			hint="COM_PRO_CRITICAL_CSS_USER_AGENT_ID_HINT"
			autocomplete="on"
		/>
		<!-- [Fields 1712] Pro_critical_url_id Field. Type: Subjectsprocriticalurlid. (custom) -->
		<field
			type="subjectsprocriticalurlid"
			name="pro_critical_url_id"
			label="COM_PRO_CRITICAL_CSS_PRO_CRITICAL_URL_ID_LABEL"
			description="COM_PRO_CRITICAL_CSS_PRO_CRITICAL_URL_ID_DESCRIPTION"
			class="list_class span8"
			multiple="false"
			default=""
			required="false"
			readonly="true"
			button="false"
		/>
		<!-- [Fields 1596] Query_params Field. Type: Subform. (joomla) -->
		<field
			type="subform"
			name="query_params"
			label="COM_PRO_CRITICAL_CSS_QUERY_PARAMS_LABEL"
			layout="joomla.form.field.subform.repeatable-table"
			multiple="true"
			buttons="add,remove"
			description="COM_PRO_CRITICAL_CSS_QUERY_PARAMS_DESCRIPTION"
			default=""
			icon="list"
			max="50"
			min="1">
			<form hidden="true" name="list_query_params_modal" repeat="true">
				<!-- [Fields 1465] Query Field. Type: Text. (joomla) -->
				<field
					type="text"
					name="query"
					label="COM_PRO_CRITICAL_CSS_QUERY_LABEL"
					maxlength="64"
					default=""
					description="COM_PRO_CRITICAL_CSS_QUERY_DESCRIPTION"
					class="text_area"
					readonly="false"
					disabled="false"
					required="false"
					filter="STRING"
					message="COM_PRO_CRITICAL_CSS_QUERY_MESSAGE"
					autocomplete="off"
				/>
				<!-- [Fields 1465] Value Field. Type: Text. (joomla) -->
				<field
					type="text"
					name="value"
					label="COM_PRO_CRITICAL_CSS_VALUE_LABEL"
					maxlength="64"
					default=""
					description="COM_PRO_CRITICAL_CSS_VALUE_DESCRIPTION"
					class="text_area"
					readonly="false"
					disabled="false"
					required="false"
					filter="STRING"
					message="COM_PRO_CRITICAL_CSS_VALUE_MESSAGE"
					autocomplete="on"
				/>
			</form>
		</field>
	</fieldset>
	
	<!-- [Interpretation 12352] Access Control Fields. -->
	<fieldset name="accesscontrol">
		<!-- [Interpretation 12354] Asset Id Field. Type: Hidden (joomla) -->
		<field
			name="asset_id"
			type="hidden"
			filter="unset"
		/>
		<!-- [Interpretation 12360] Rules Field. Type: Rules (joomla) -->
		<field
			name="rules"
			type="rules"
			label="Permissions in relation to this css"
			translate_label="false"
			filter="rules"
			validate="rules"
			class="inputbox"
			component="com_pro_critical"
			section="css"
		/>
	</fieldset>
</form>